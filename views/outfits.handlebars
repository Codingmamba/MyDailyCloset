<h4 id="saveoutfit">


  MY OUTFITS</h4>

<div id="savebox" class="container">
  {{#each outfits}}
  <div style="float:left" class="imgDiv">
    <div>
      <img src="{{outfitspath}}" alt="outfit" class="closetImg" height="200" width="200">
    </div>

    <button type="button" class="btn btn-default change-fav" data-id="{{this.outfitsID}}" aria-label="Favorite" name="favorite"
      {{#if favorite}} value="1">
      <span class="glyphicon glyphicon-star" aria-hidden="true"></span>

      {{else}} value="0">
      <span class="glyphicon glyphicon-star-empty" aria-hidden="true"></span>{{/if}}
    </button>

    <button type="button" class="btn btn-default delete" data-id="{{this.outfitsID}}" aria-label="Delete">
      <span class="glyphicon glyphicon-remove" aria-hidden="true"></span>
    </button>

  </div>
  {{/each}}
</div>

<br>
<hr>
<br>

<script src="/assets/js/outfitsjs.js"></script>