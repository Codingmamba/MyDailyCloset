<div class="container">
<nav class="navbar navbar-default">
  <div class="container-fluid">
    <div class="navbar-header">
      <a class="navbar-brand" href="#">MyDailyCloset</a>
    </div>
    <ul class="nav navbar-nav navbar-right">
      <li class="active"><a href="#">Home</a></li>
      <li><a href="#">Page 1</a></li>
      <li><a href="#">Page 2</a></li>
      <li><a href="#">Page 3</a></li>
      <li><a href="#">Page 4</a></li>
    </ul>
  </div>
</nav>
</div>

<div class="row">
 <div class="col-md-12"> 
  <h4 id="mycloset">MY OUTFITS</h4>
 </div>
</div>

  {{#each outfits}}
    <div style="float:left">
      <div>
        <img src="{{outfitspath}}" alt="outfit" height="400" width="400">
      </div>

        <button type="button" class="btn btn-default change-fav" data-id="{{this.outfitsID}}" aria-label="Favorite"
        name="favorite"

        {{#if favorite}} value="1"> <span class="glyphicon glyphicon-star" aria-hidden="true"></span>

        {{else}} value="0"><span class="glyphicon glyphicon-star-empty" aria-hidden="true"></span>{{/if}}
        </button>

        <button type="button" class="btn btn-default delete" data-id="{{this.outfitsID}}" aria-label="Delete">
        <span class="glyphicon glyphicon-remove" aria-hidden="true"></span>
        </button>

    </div>
  {{/each}}

    <script src="/assets/js/outfitsjs.js"></script>
 

